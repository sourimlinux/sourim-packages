name="tar"
version="1.35"
depends="glibc"
maintainer="Andrey Stekolnikov <honakac@yandex.ru>"
source=(
    "http://mirror.checkdomain.de/gnu/tar/tar-$version.tar.xz"
)

package() {
    cd $name-$version
    FORCE_UNSAFE_CONFIGURE=1  \
        ./configure --prefix=/usr
    make
    make install DESTDIR=$1
}
